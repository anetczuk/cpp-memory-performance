#!/bin/bash

##
## This file is automatically generated from "@GENERATOR_FILE@". Changes will be lost.
##

set -eu

SCRIPT_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"


exec=$SCRIPT_DIR/@EXECUTABLE@

output_dir=$SCRIPT_DIR/cachegrind

mkdir -p $output_dir

cd $output_dir


#### output description:
#### Ir   -- number of executed instructions
#### I1mr -- L1 instruction cache read misses
#### ILmr -- last level instruction cache read misses
#### Dr   -- number of memory reads
#### D1mr -- L1 data cache read misses
#### DLmr -- last level data cache read misses
#### Dw   -- number of memory writes
#### D1mw -- L1 data cache write misses
#### DLmw -- last level data cache write misses
#### Bc   -- number of executed conditional branches
#### Bcm  -- number of mispredicted conditional branches
#### Bi   -- number of executed indirect branches
#### Bim  -- number of mispredicted indirect branches

#### additional information is availble by executing "cg_annotate" with cachegrind output file


valgrind --tool=cachegrind $exec $@

